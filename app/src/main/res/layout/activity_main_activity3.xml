<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.julio.my_app_correction.MainActivity3"
    android:orientation="vertical"
    android:weightSum="1"
    android:background="#fff"
    android:longClickable="true">




    <RatingBar
        android:id = "@+id/myRatingBar"
        style="?android:attr/ratingBarStyleSmall"
        android:layout_width = "wrap_content"
        android:layout_height = "wrap_content"
        android:numStars = "5"
        android:rating="5.0"
        android:stepSize = "1.0"
        android:layout_gravity="center_horizontal" />

    <RatingBar
        android:id = "@+id/ratingbar"
        android:layout_width = "wrap_content"
        android:layout_height = "wrap_content"
        android:numStars = "5"
        android:rating="2.0"
        android:stepSize = "1.0"
        style="?android:attr/ratingBarStyleSmall"
        android:layout_gravity="center_horizontal"></RatingBar>

    <RatingBar
        android:id = "@+id/indicator_ratingbar"
        style="?android:attr/ratingBarStyleSmall"
        android:layout_marginLeft = "2dip"
        android:layout_width = "wrap_content"
        android:numStars = "5"
        android:rating="2.0"
        android:stepSize = "1.0"
        android:layout_height = "wrap_content"
        android:layout_gravity = "center_horizontal"  />
    <LinearLayout
        android:layout_width = "wrap_content"
        android:layout_height = "wrap_content"  >
        <RatingBar
            android:id = "@+id/myRatingBarultimo"
            style="?android:attr/ratingBarStyleSmall"
            android:layout_width = "wrap_content"
            android:layout_height = "wrap_content"
            android:numStars = "5"
            android:rating="2.0"
            android:stepSize = "1.0"
            android:layout_gravity="center_horizontal" />

    </LinearLayout>

    <RatingBar
        android:id = "@+id/ratingbarsdf"

        style="?android:attr/ratingBarStyleSmall"
        android:layout_width = "wrap_content"
        android:layout_height = "wrap_content"
        android:numStars = "5"
        android:rating="5.0"
        android:stepSize = "1.0"
        android:layout_gravity="center_horizontal"></RatingBar>

    <Button
        android:layout_width="137dp"
        android:layout_height="wrap_content"
        android:text="New Button"
        android:id="@+id/button"
        android:layout_gravity="center_horizontal"
        android:layout_weight="0.15"
        android:background="#ff82a3ff"
        android:layout_marginTop="40dp"
        android:elegantTextHeight="false"
        android:elevation="5dip" />

    <Button
        android:layout_width="140dp"
        android:layout_height="wrap_content"
        android:text="New Button"
        android:id="@+id/button2"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="20dp"
        android:background="#ffbeff99"
        android:layout_weight="0.16"
        android:elevation="10dp" />

    <Button
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="New Button"
        android:id="@+id/button3"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="20dp"
        android:background="#ffff9b71"
        android:layout_weight="0.16" />

    <Button
        android:layout_width="162dp"
        android:layout_height="wrap_content"
        android:text="New Button"
        android:id="@+id/button4"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="20dp"
        android:layout_weight="0.15"
        android:background="#fff" />

    <EditText
        android:id="@+id/edit1"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="edith_text_uno"
        android:inputType="text"
        android:background="@drawable/edith_registro_change_dos"
        />

    <EditText
        android:id="@+id/edit2"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="edith_text_dos"
        android:inputType="text"
        android:background="@drawable/edith_registro_change_dos"
        />


</LinearLayout>
